extends layout/main

block variables
  - const title = 'История заказов'
  - const mainPage = false
  - const activeLink = 'Заказы (2)'

//- ИСТОРИЯ ЗАКАЗОВ
block content
  main.page-content.page-content--personal-area
      .page-inner.page-inner--w2
        .page-heading
          .row
            .col-md-4 
              .mb-4.d-lg-none
                a.link.link--gray_border(href="dashboard.html") Личный кабинет
              .page-heading-title.page-heading-title_personal.uppercase.d-none.d-lg-block= activeLink           

            .col-md-8
              //- .page-heading-title1.border-bottom-gray.only-desktop &nbsp;

        .row
          .col-md-3.d-none.d-lg-block 
            +menu('personal-menu', personalMenuData, activeLink)
          .col-lg-1
          .col-12.col-md-8.offset-md-2.col-lg-8.offset-lg-0.col-xl-8.border-md-top-gray 
            include modules/personal/history  