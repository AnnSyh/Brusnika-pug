    link(rel='stylesheet', type='text/css', media='screen', href='../css/plugins/datepicker.min.css')
    link(rel='stylesheet', type='text/css', media='screen', href='../css/project.css?v=11312')
    link(rel='stylesheet', type='text/css', media='screen', href='../css/project-ann.css?v=797191')
    link(rel='stylesheet', type='text/css', media='screen', href='../css/project.responsive.css?v=724611')
    link(rel='stylesheet', type='text/css', media='screen', href='../css/project.responsive-ann.css?v=724611')
    //- CSS
  body(class={'body-main':mainPage,
				'body-inner':innerPage})

    .page
      include ../mixins/util
      include ../modules/page-note
      include ../modules/header

      block content

      include ../modules/footer
    include ../modules/footer-popups
    include ../modules/footer-scripts 